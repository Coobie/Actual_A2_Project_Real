<h1>Search results</h1>
<p><%= link_to "back", new_search_path %> </p>

	<h2>User Search Results</h2>
<% if @search.search_users.empty? %>
	<p>No results found for users </p>
<% else %>

<table class="table1" width="35%" >
	<tr>
		<th>First Name </th>
		<th>Last Name </th>
		<th>Email  </th>
		<th>Action  </th>

	</tr>
<% @search.search_users.each do |c|  %>
	<td><%= link_to "#{c.first_name}",user_path(c) %> </td>
	<td><%= link_to "#{c.last_name}",user_path(c) %></td>
	<td><%= link_to "#{c.email}",user_path(c) %></td>
	<td><%= link_to "Edit ", edit_user_path(c) %></td>
</tr>
<% end %>
<% end %>
